    script(type='text/javascript').
      let sketch = p => {
        let t=0
        let count=1
        let angle=0
        let time = {};
        function d() {
          const d = new Date();
          const h = d.getHours();
          const m = d.getMinutes();
          const s = d.getSeconds();
          const result = '+00:' + h + ':' + m + ':' + s;
          time.h = h;
          time.m = m;
          time.s = s;
          return result;
        }
        p.setup = () => {
          p.createCanvas(360, 360);
        }
        p.draw = () => {
          p.clear()
          const w = 360;
          const h = 360;
          p.background('rgba(255, 255, 255, 0)');

          // 十字線
          p.stroke('rgba(7, 233, 210, .1)');
          p.strokeWeight(1);
          p.line(0, h / 2, w, h / 2);
          p.line(w / 2, 0, w / 2, h);

          // 旋轉圓
          let r1 = w / 2.5;
          p.stroke('rgba(7, 233, 210, .3)');
          p.strokeWeight(2);
          p.push();
          p.translate(w / 2, h / 2);
          p.translate(w / 100 / 50, h / 100 / 50);
          p.beginShape();
          p.fill('rgba(7, 233, 210, .3)');

          p.arc(0, 0, 150, 150, p.QUARTER_PI + t / 50, p.PI - p.QUARTER_PI * 2 + t / 50); 
          p.noFill();
          
          // 內刻度
          let r2 = w / 5;
          let num2 = w / 3;
          p.strokeWeight(2);
          p.stroke('rgba(7, 233, 210, .1)');
          for (let i = 0; i < num2; i++) {
            let cp = p.TWO_PI / num2 * i + t / 1000;
            let x1 = r2 * p.cos(cp);
            let y1 = r2 * p.sin(cp);
            let x2 = (r2 + 5) * p.cos(cp);
            let y2 = (r2 + 5) * p.sin(cp);
            p.line(x1, y1, x2, y2);
          }
          p.stroke('rgba(7, 233, 210, .3)');
          p.strokeWeight(2);
          for (let i = 0; i < 8; i++) {
            let cp = p.TWO_PI / 8 * i + t / 2000;
            let x1 = (r2 - 3.5) * p.cos(cp);
            let y1 = (r2 - 3.5) * p.sin(cp);
            let x2 = (r2 + 7) * p.cos(cp);
            let y2 = (r2 + 7) * p.sin(cp);
            p.line(x1, y1, x2, y2);
          }

          // 外刻度
          let r3 = w / 3.5;
          let num3 = 60;
          p.stroke('rgba(7, 233, 210, .1)');
          p.strokeWeight(2);
          for (let i = 0; i < num3; i++) {
            let cp = p.TWO_PI / num3 * i;
            let x1 = r3 * p.cos(cp);
            let y1 = r3 * p.sin(cp);
            let x2 = (r3 + 7) * p.cos(cp);
            let y2 = (r3 + 7) * p.sin(cp);
            p.line(x1, y1, x2, y2);
          }
          p.strokeWeight(2);
          p.stroke('rgba(7, 233, 210, .3)');
          for (let i = 0; i < 4; i++) {
            let cp = p.TWO_PI / 4 * i;
            let x1 = (r3 - 6) * p.cos(cp);
            let y1 = (r3 - 6) * p.sin(cp);
            let x2 = (r3 + 12) * p.cos(cp);
            let y2 = (r3 + 12) * p.sin(cp);
            p.line(x1, y1, x2, y2);
          }

          // 內兩側弧
          const r4 = w / 1.2;
          const angle4 = angle / 80 * (p.PI / 180) + t / 500;
          p.strokeWeight(4);
          p.stroke('rgba(7, 233, 210, .3)');
          p.strokeCap(p.SQUARE);
          p.arc(0, 0, r4, r4, 0 + angle4, p.HALF_PI + angle4);
          p.arc(0, 0, r4, r4, p.PI + angle4, p.PI + p.HALF_PI + angle4);
          p.pop();
          t++;
          angle += count;
          if (angle === 300) {
            count = -1;
          } else if (angle === 0) {
            count = 1;
          } 
        };
      };
